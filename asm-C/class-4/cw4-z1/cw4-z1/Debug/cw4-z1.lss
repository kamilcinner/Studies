                                 .org 0						; wymuszenie pocztku nastpnych rozkazw od adresu 0x000
000000 940c 0004                 jmp RESET					; pod adresem 0x000 znajduje si wektor przerwania inicjalizacyjnego
                                 							; skok bezporedni do podprogramu obsugi tego przerwania
000002 940c 0017                 jmp EXT_INT0				; pod adresem 0x002 znajduje si wektor przerwania zewntrznego INT0
                                 							; (poprzednia instrukcja skoku zajmuje dwa sowa i std przesunicie w pamici)
                                 							; skok bezporedni do podprogramu obsugi tego przerwania
                                 
                                 RESET:						; podprogram obsugi przerwania inicjalizacyjnego
000004 e008                      	ldi R16, HIGH(RAMEND)	; inicjalizacja wskanika stosu
000005 bf0e                      	out SPH, R16
000006 e50f                      	ldi R16, LOW(RAMEND)
000007 bf0d                      	out SPL, R16
000008 9478                      	sei						; zezwolenie na obsug przerwa
000009 ef0f                      	ser R16					; ustaw wszystkie bity rejestru R16
00000a bb0a                      	out DDRA, R16			; ustaw wszystkie piny PA jako linie wyjciowe
00000b bb07                      	out DDRB, R16			; ustaw wszystkie piny PB jako linie wyjciowe
00000c ef00                      	ldi R16, 0b11110000		; ustaw 4 najstarsze i wyzeruj 4 najmodsze bity PA
00000d bb0b                      	out PORTA, R16
                                 
00000e b705                      	in R16, MCUCR			; wczytaj rejestr MCUCR do rejestru R16
00000f 6002                      	ori R16, 0b00000010		; ustaw R16.1=1
000010 7f0e                      	andi R16, 0b11111110	; ustaw R16.1=0
000011 bf05                      	out MCUCR, R16			; ustaw tryb 'falling edge' dla przerwania INT0
                                 							; czyli przerwanie zostanie wywoane gdy pin PD.2 zmieni swj stan z wysokiego na niski (1 -> 0)
                                 
000012 b70b                      	in R16, GICR			; wczytaj rejestr GICR do rejestru R16
000013 6400                      	ori R16, 0b01000000		; ustaw R16.6=1
000014 bf0b                      	out GICR, R16			; odblokuj przerwanie INT0
                                 
000015 940c 0022                 	jmp main				; skocz do programu gwnego
                                 
                                 EXT_INT0:					; podrpogram obsugi przerwania INT0
000017 94f8                      	cli						; zablokuj obsug przerwa
000018 930f                      	push R16				; zabezpiecz zawarto rejestru R16 na stosie
000019 b70f                      	in R16, SREG			; wczytaj rejestr SREG do rejestru R16
00001a 930f                      	push R16				; zabezpiecz zawarto rejestru SREG na stosie
                                 
00001b b30b                      	in R16, PORTA			; wczytaj reejstr PORTA do rejestru R16
00001c 9500                      	com R16					; neguj bity rejestru R16
00001d bb0b                      	out PORTA, R16			; zapisz zawarto rejestru R16 w rejestrze PORTA
                                 
00001e 910f                      	pop R16					; zdejmij ze stosu pierwotn warto SREG i zapisz w R16
00001f bf0f                      	out SREG, R16			; przywr zachowany rejestr SREG
000020 910f                      	pop R16					; przywr zachowany rejestr R16
000021 9518                      	reti					; powr z podprogramu obsugi przerwania INT0
                                 							; jednoczenie ustawiajc flag zezwolenia na przerwania w SREG
                                 
                                 main:						; program gwny
000022 e000                      	ldi R16, 0				; wczytaj warto 0 do rejestru R16
000023 ef1e                      	ldi R17, -2				; wczytaj warto -2 do rejestru R17
000024 0f10                      	add	R17, R16			; dodaj warto 0 do wartoci -2
000025 d007                      	rcall delayNop256		; wywoj podprogram opniajcy
000026 f014                      	brbs 4, negative		; skocz do etykiety jeli wynik by ujemny
                                 nonNegative:
000027 e505                      	ldi R16, 0b01010101		; wczytaj warto 01010101 (bin) do rejestru R16 (wynik nieujemny)
000028 c001                      	rjmp continue			; pomi ustawianie wartoci dla wyniku ujemnego
                                 negative:
000029 ea0a                      	ldi R16, 0b10101010		; wczytaj warto 10101010 (bin) do rejestru R16 (wynik ujemny)
                                 continue:
00002a bb08                      	out PORTB, R16			; zapisz warto rejestru R16 w rejestrze PORTB
00002b 940c 0022                 	jmp main				; wr na pocztek programu gwnego
                                 
